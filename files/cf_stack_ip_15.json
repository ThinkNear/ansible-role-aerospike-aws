        "Description": "A static aerospike network interface.",
        "GroupSet" : [ { "Ref" : "InstanceSecurityGroup" }],
        "PrivateIpAddress": { "Fn::Select" : [ "6", {"Ref" : "PrivateStaticIPs"} ] },
        "SubnetId": { "Ref" : "AerospikeSubnet" }
      },
      "DependsOn": "InstanceSecurityGroup"
    },

    "NetworkInterface8" : {
      "Type" : "AWS::EC2::NetworkInterface",
      "Properties" : {
        "Description": "A static aerospike network interface.",
        "GroupSet" : [ { "Ref" : "InstanceSecurityGroup" }],
        "PrivateIpAddress": { "Fn::Select" : [ "7", {"Ref" : "PrivateStaticIPs"} ] },
        "SubnetId": { "Ref" : "AerospikeSubnet" }
      },
      "DependsOn": "InstanceSecurityGroup"
    },

    "NetworkInterface9" : {
      "Type" : "AWS::EC2::NetworkInterface",
      "Properties" : {
        "Description": "A static aerospike network interface.",
        "GroupSet" : [ { "Ref" : "InstanceSecurityGroup" }],
        "PrivateIpAddress": { "Fn::Select" : [ "8", {"Ref" : "PrivateStaticIPs"} ] },
        "SubnetId": { "Ref" : "AerospikeSubnet" }
      },
